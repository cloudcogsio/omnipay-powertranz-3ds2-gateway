{
  "name": "cloudcogsio/omnipay-powertranz-3ds2-gateway",
  "type" : "library",
  "license" : "MIT",
  "authors" : [
    {"name" : "Ricardo Assing", "email" : "ricardo@tsiana.ca"}
  ],
  "keywords" : [
    "cloudcogs",
    "cloudcogs.io",
    "powertranz",
    "3ds",
    "api",
    "payment-gateway",
    "omnipay"
  ],
  "description" : "Powertranz 3DS2 Payment Gateway Driver for Omnipay",
  "minimum-stability" : "dev",
  "prefer-stable": true,
  "require": {
    "php": "8.*",
    "omnipay/common" : "^3.4",
    "ext-json": "*",
    "ramsey/uuid": "^4.9",
    "league/iso3166": "^4.3",
    "alcohol/iso4217": "^4.2",
    "symfony/http-client": "^5.4",
    "php-http/httplug": "^2.4",
    "nyholm/psr7": "^1.8",
    "brick/math": "^0.10.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "autoload" : {
    "psr-4" : {
      "Omnipay\\PowerTranz\\" : "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Omnipay\\PowerTranz\\Tests\\": "tests"
    }
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    },
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-main": "1.0.x-dev"
    }
  }
}